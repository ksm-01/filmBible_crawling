<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dev.benew.filmbiblecrawling.mapper.ChannelMapper">
    <resultMap id="channelMap" type="dev.benew.filmbiblecrawling.dto.ChannelDto">
        <id column="fb_channel_id" property="channelId" />
        <result column="fb_channel_name" property="channelName" />
        <result column="fb_channel_subscriber" property="channelSubscriber" />
        <result column="fb_channel_reg_date" property="channelRegDate" />
    </resultMap>

    <insert id="saveChannel">
        insert into FB_CHANNEL (
                fb_channel_id,
                fb_channel_name,
                fb_channel_subscriber
        ) values (
                #{channelId},
                #{channelName},
                #{channelSubscriber}
        )
    </insert>

</mapper>